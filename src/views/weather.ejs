            const d = new Date();
            const date = d.getDate();
            const month = d.getMonth();
            const year = d.getFullYear().toString();
            const hours = d.getHours();
            return `${date} | ${monthname(month + 1)} | ${year}`;
        }

        const getcurrtime = () => {
            const d = new Date();
            let hours = d.getHours().toString();
            let minute = d.getMinutes();
            let strmin;
            if (minute % 10 == 0 && minute < 10) {
                strmin = `0 ${minute}`;
            } else {
                strmin = minute.toString();
            }

            let meridiem = "AM";
            if (hours > 12) {
                hours %= 12;
                meridiem = "PM";
            }
            else {
                meridiem = "PM";
            }

            console.log("strmin", strmin);
            return `${hours} : ${strmin} ${meridiem}`;
        }
        console.log(getcurrtime());
