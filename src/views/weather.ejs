                console.log(lat + " " + lon);
                const data = { lat, lon };

                const options = {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(data)
                };
                fetch('/location', options);
            });
